{
  "name": "connect",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/connect.umd.js",
  "module": "dist/connect.es5.js",
  "typings": "dist/types/connect.d.ts",
  "files": [
    "dist"
  ],
  "author": "Will Ockelmann-Wagner <wow@carbonfive.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "npm-run-all -c lint:*",
    "lint:eslint": "eslint --fix '**/*.{ts,tsx}'",
    "lint:prettier": "prettier --write '**/*.{tsx,ts,css,html,md,json}'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "format": "prettier --write '**/*.{tsx,ts,css,html,md,json}'"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@rxreact/core": ">= 5.0",
    "react": ">= 16.5.2",
    "rxjs": ">= 6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@rxreact/core": "^5.1.1",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^10.11.0",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^1.0.1",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "16",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "rxjs": "^6.5.4",
    "semantic-release": "^17.2.3",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^7.0.1",
    "typedoc": "^0.15.8",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
